@model IEnumerable<DAL.TestPaper>

@{
    ViewBag.Title = "Test Papers";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Test Papers</h2>

<p>
    @Html.ActionLink("Create New", "Index")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Name)
        </th>
        @*<th>
                @Html.DisplayNameFor(model => model.IsActive)
            </th>*@
        <th>
            @Html.DisplayNameFor(model => model.CreatedOn)
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.ActionLink(item.Name, "ViewAllQuestionsofTestPaper", new { testPaperID = item.Id }) 
                @*@Html.DisplayFor(modelItem => item.Name)*@
            </td>
            @*<td>
                    @Html.DisplayFor(modelItem => item.IsActive)
                </td>*@
            <td>
                @Html.DisplayFor(modelItem => item.CreatedOn)
            </td>
            <td>
                @*@Html.ActionLink("Edit", "Edit", new { id = item.Id }) |*@
                @Html.ActionLink("Add Questions", "TestQuestions", new { testPaperID = item.Id }) |
                @*@Html.ActionLink("Delete", "Delete", new { id = item.Id })*@
            </td>
        </tr>
    }
</table>
